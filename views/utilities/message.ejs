
<% if (errors.length > 0) { %>

    <div class="alert alert-danger">
    
        <% errors.forEach(item => { %>
            <%= item.msg %> <br> 
        <% }) %>
    
    </div>
    
    <% } %>
    
    <% if (success) { %>
        
        <div class="alert alert-success">
            <%= success %> 
        </div>
        
    <% } %>
    